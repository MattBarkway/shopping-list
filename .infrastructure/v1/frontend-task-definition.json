[
  {
    "volumesFrom": [],
    "portMappings": [
      {
        "hostPort": 3000,
        "containerPort": 3000,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "mountPoints": [],
    "name": "frontend",
    "environment": [
      { "name": "DATABASE", "value": "sl" }
    ],
    "image": "796680308813.dkr.ecr.eu-west-1.amazonaws.com/sl-frontend-ecr:latest",
    "cpu": 256,
    "memory": 512,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/sl-logs",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "frontend"
      }
    }
  }
]
