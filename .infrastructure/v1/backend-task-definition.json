[
  {
    "volumesFrom": [],
    "portMappings": [
      {
        "hostPort": 8000,
        "containerPort": 8000,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "mountPoints": [],
    "name": "backend",
    "environment": [
      { "name": "DATABASE", "value": "sl" },
      { "name": "DATABASE2", "value": "sl" },
      { "name": "DB_USER", "value": "postgres" },
      { "name": "DB_PASSWORD", "value": "postgres" },
      { "name": "DB_HOST", "value": "database" },
      { "name": "DB_PORT", "value": "5432" },
      { "name": "ASYNC_DRIVER", "value": "asyncpg" },
      { "name": "DRIVER", "value": "psycopg2" },
      { "name": "SENDGRID_KEY", "value": "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" },
      { "name": "FROM_EMAIL", "value": "contact@mattbarkway.dev" },
      { "name": "HOST", "value": "0.0.0.0:8000" },
      { "name": "SALT", "value": "M@UXRHNxZPQe!&92zscGmm*R" },
      { "name": "SECRET_KEY", "value": "1234" },
      { "name": "MAX_VERIFY_AGE_SECONDS", "value": "500" },
      { "name": "ACCESS_TOKEN_EXPIRE_MINUTES", "value": "30" },
      { "name": "ALGORITHM", "value": "HS256" },
      { "name": "ALLOWED_HOST", "value": "*" }
    ],
    "image": "796680308813.dkr.ecr.eu-west-1.amazonaws.com/sl-backend-ecr:latest",
    "cpu": 256,
    "memory": 512,
    "logConfiguration": {
